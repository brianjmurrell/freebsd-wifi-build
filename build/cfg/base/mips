# Build environment and configuration for mips32 big-endian

BUILDNAME=mips
TARGET=mips
TARGET_ARCH=mips
TARGET_CPUTYPE=mips32
LOCAL_DIRS="tools/tools/ath tools/tools/net80211/wlanstats tools/tools/net80211/wlanwatch tools/bsdbox"
LOCAL_TOOL_DIRS="tools/bsdbox/"

X_PORTBUILD_PLATFORM="mips"

# Default packages required
X_DEFAULT_PACKAGELIST="dropbear dnsmasq lua bsdtools"

# Example using cross toolchain for base system
TARGET_CROSS_TOOLCHAIN="mips-gcc"
